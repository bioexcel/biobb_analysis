{
    "_id": "biobb_analysis",
    "desc": "Biobb_analysis is the Biobb module collection to perform analysis of molecular dynamics simulations",
    "github": "https://github.com/bioexcel/biobb_analysis",
    "readthedocs": "https://biobb-analysis.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_analysis",
    "docker": "https://quay.io/biocontainers/biobb_analysis:5.1.1--pyhdfd78af_0",
    "singularity": "https://depot.galaxyproject.org/singularity/biobb_analysis:5.1.1--pyhdfd78af_0",
    "version": "5.1.1",
    "rest": true,
    "tools": [
        {
            "block": "GMXCluster",
            "tool": "gmx cluster",
            "desc": "Wrapper of the GROMACS cluster module for clustering structures from a given GROMACS compatible trajectory.",
            "exec": "gmx_cluster",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_cluster",
            "rest": true
        },
        {
            "block": "GMXRms",
            "tool": "gmx rms",
            "desc": "Wrapper of the GROMACS module for calculating the Root Mean Square deviation (RMSd) of a given GROMACS compatible trajectory.",
            "exec": "gmx_rms",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_rms",
            "rest": true
        },
        {
            "block": "GMXRgyr",
            "tool": "gmx gyrate",
            "desc": "Wrapper of the GROMACS gyrate module for computing the radius of gyration (Rgyr) of a molecule about the x-, y- and z-axes, as a function of time, from a given GROMACS compatible trajectory.",
            "exec": "gmx_rgyr",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_rgyr",
            "rest": true
        },
        {
            "block": "GMXEnergy",
            "tool": "gmx energy",
            "desc": "Wrapper of the GROMACS energy module for extracting energy components from a given GROMACS energy file.",
            "exec": "gmx_energy",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_energy",
            "rest": true
        },
        {
            "block": "GMXImage",
            "tool": "gmx trjconv",
            "desc": "Wrapper of the GROMACS trjconv module for correcting periodicity (image) from a given GROMACS compatible trajectory file.",
            "exec": "gmx_image",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_image",
            "rest": true
        },
        {
            "block": "GMXTrjconvStr",
            "tool": "gmx trjconv",
            "desc": "Wrapper of the GROMACS trjconv module for converting between GROMACS compatible structure file formats and/or extracting a selection of atoms.",
            "exec": "gmx_trjconv_str",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_trjconv_str",
            "rest": true
        },
        {
            "block": "GMXTrjconvStrEns",
            "tool": "gmx trjconv",
            "desc": "Wrapper of the GROMACS trjconv module for extracting an ensemble of frames containing a selection of atoms from GROMACS compatible trajectory files.",
            "exec": "gmx_trjconv_str_ens",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_trjconv_str_ens",
            "rest": true
        },
        {
            "block": "GMXTrjconvTrj",
            "tool": "gmx trjconv",
            "desc": "Wrapper of the GROMACS trjconv module for converting between GROMACS compatible trajectory file formats and/or extracts a selection of atoms.",
            "exec": "gmx_trjconv_trj",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/gromacs.html#module-gromacs.gmx_trjconv_trj",
            "rest": true
        },
        {
            "block": "CpptrajAverage",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for calculating a structure average of a given cpptraj compatible trajectory.",
            "exec": "cpptraj_average",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_average",
            "rest": true
        },
        {
            "block": "CpptrajBfactor",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for calculating the Bfactor fluctuations of a given cpptraj compatible trajectory.",
            "exec": "cpptraj_bfactor",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_bfactor",
            "rest": true
        },
        {
            "block": "CpptrajRms",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for calculating the Root Mean Square deviation (RMSd) of a given cpptraj compatible trajectory.",
            "exec": "cpptraj_rms",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_rms",
            "rest": true
        },
        {
            "block": "CpptrajRmsf",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for calculating the Root Mean Square fluctuations (RMSf) of a given cpptraj compatible trajectory.",
            "exec": "cpptraj_rmsf",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_rmsf",
            "rest": true
        },
        {
            "block": "CpptrajRgyr",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for computing the radius of gyration (Rgyr) from a given cpptraj compatible trajectory.",
            "exec": "cpptraj_rgyr",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_rgyr",
            "rest": true
        },
        {
            "block": "CpptrajDry",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for dehydrating a given cpptraj compatible trajectory stripping out solvent molecules and ions.",
            "exec": "cpptraj_dry",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_dry",
            "rest": true
        },
        {
            "block": "CpptrajStrip",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for stripping a defined set of atoms (mask) from a given cpptraj compatible trajectory.",
            "exec": "cpptraj_strip",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_strip",
            "rest": true
        },
        {
            "block": "CpptrajSnapshot",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for extracting a particular snapshot from a given cpptraj compatible trajectory.",
            "exec": "cpptraj_snapshot",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_snapshot",
            "rest": true
        },
        {
            "block": "CpptrajSlice",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for extracting a particular trajectory slice from a given cpptraj compatible trajectory.",
            "exec": "cpptraj_slice",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_slice",
            "rest": true
        },
        {
            "block": "CpptrajConvert",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for converting between cpptraj compatible trajectory file formats and/or extracting a selection of atoms or frames.",
            "exec": "cpptraj_convert",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_convert",
            "rest": true
        },
        {
            "block": "CpptrajMask",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for extracting a selection of atoms from a given cpptraj compatible trajectory.",
            "exec": "cpptraj_mask",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_mask",
            "rest": true
        },
        {
            "block": "CpptrajImage",
            "tool": "Ambertools cpptraj",
            "desc": "Wrapper of the Ambertools Cpptraj module for correcting periodicity (image) from a given cpptraj trajectory file.",
            "exec": "cpptraj_image",
            "docs": "https://biobb-analysis.readthedocs.io/en/latest/ambertools.html#module-ambertools.cpptraj_image",
            "rest": true
        }
    ],
    "dep_pypi": [
        "install_requires=['biobb_common==5.1.0']",
        "python_requires='>=3.9'"
    ],
    "dep_conda": [
        "python >=3.9",
        "biobb_common ==5.1.0",
        "gromacs >=2024.5",
        "ambertools >=22.5"
    ],
    "keywords": [
        "analysis",
        "ambertools",
        "gromacs"
    ]
}